{
  "template": "mainTpl.dust",
  "selector": "main.mainView",
  "attributes": {
    "className": "mainClass container-fluid",
    "tagName": "main"
  },
  "sections": [
    {
      "id": "splash",
      "template": "splashTpl.dust",
      "attributes": {
        "className": "splash"
      },
      "selector": ".splash"
    },
    {
    "container": {
      "attributes": {
        "className": "content"
     }
    },
    "children": [
        {
            "id": "title",
            "selector": "section.title",
            "template": "titleTpl.dust",
            "tagName": "section",
            "attributes": {
              "className": "titleView"
            }
        },
        {
            "id": "intro",
            "selector": "section.intro",
            "template": "introTpl.dust",
            "tagName": "section",
            "attributes": {
              "className": "introView"
            }
        }
      ]
    }]
}
