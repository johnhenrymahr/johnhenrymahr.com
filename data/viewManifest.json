{
  "id": "main",
  "template": "mainTpl.dust",
  "selector": "main.mainView",
  "childViewContainer": ".contentContainer",
  "attributes": {
    "className": "main container-fluid",
    "tagName": "main"
  },
  "sections": [
    {
      "id": "header",
      "selector": "header",
      "template": "header.dust",
      "attributes": {
        "tagName": "header",
        "className": "header"
      }
    },
    {
      "id": "content",
      "selector": ".content",
      "attributes": {
        "className": "content"
      },
      "children": [
        {
          "id": "core",
          "selector": "section.core",
          "template": "coreTpl.dust",
          "attributes": {
            "tagName": "section",
            "className": "core",
            "id": "core",
            "tabindex": "0"
          }
        }
      ]
    },
    {
      "id": "details",
      "selector": ".details",
       "template": "details.dust",
       "attributes": {
        "id": "details",
        "className": "details onLoad",
        "tagName": "section"
      }
    },
    {
      "id": "tech",
      "selector": ".tech",
      "template": "tech.dust",
      "attributes": {
        "id": "tech",
        "className": "tech onLoad",
        "tagName": "section"
      }
    },
    {
      "id": "contact",
      "selector": ".contact",
      "template": "contactTpl.dust",
      "attributes": {
        "id": "contact",
        "className": "contact col-xs-12 onLoad",
        "tagName": "section"
      }
    },
    {
      "id": "footer",
      "selector": ".footer",
      "template": "footer.dust",
      "attributes": {
        "className": "footer",
        "tagName": "footer"
      }
    }
  ]
}